def block articulatedThings

artMelody -> [e, e., f, f!, g, g^, a, a`, b, b_, c, c~]
artChord -> [E, E., F, F!, G, G^, A, A`, B, B_, C, C~]

articulatedThings {
    { 5 } => octave

    //First test the melody articulations
    artMelody*<e>
    artMelody*<q>

    //Test chord articulations
    artChord*<e>
    artChord*<q>
}